{
  "name": "backendpdi",
  "version": "1.0.0",
  "description": "backend proyecto pdi",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "deploy": "node --max-old-space-size=3072 -r dotenv/config -e 'require(\"child_process\").execSync(\"npm run migrate\");' && npm start",
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrate": "npx prisma migrate deploy",
    "prisma:generate": "node --max-old-space-size=3072 node_modules/.bin/prisma generate"
  },
  "author": "Jack Torres",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  }
}
